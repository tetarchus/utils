{
  "name": "@tetarchus/utils",
  "version": "0.3.0",
  "description": "Shared Utility Packages",
  "license": "MIT",
  "keywords": [
    "utilities",
    "utils"
  ],
  "author": "Tetarchus <contact@tetbot.app>",
  "repository": {
    "type": "github",
    "url": "git+https://github.com/tetarchus/utils.git"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./remix": {
      "types": "./dist/remix/index.d.ts",
      "import": "./dist/remix/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.cjs"
    },
    "./typeguards": {
      "types": "./dist/typeguards/index.d.ts",
      "import": "./dist/typeguards/index.js",
      "require": "./dist/typescript/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "release": "npm run build && npm publish --access public",
    "test": "vitest run --config ./vitest.config.ts",
    "test:ui": "vitest dev --config ./vitest.config.ts --ui --watch",
    "tsc": "tsc"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "lodash-es": "4.17.22"
  },
  "devDependencies": {
    "@remix-run/node": "2.17.4",
    "@types/lodash-es": "4.17.12",
    "@types/node": "24.10.9",
    "@vitest/coverage-istanbul": "4.0.17",
    "@vitest/ui": "4.0.17",
    "jsdom": "27.4.0",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vitest": "4.0.17"
  }
}
