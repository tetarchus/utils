{
  "name": "@tetarchus/utils",
  "version": "0.1.3",
  "description": "Shared Utility Packages",
  "license": "MIT",
  "keywords": [
    "utilities",
    "utils"
  ],
  "author": "Tetarchus <contact@tetbot.app>",
  "repository": {
    "type": "github",
    "url": "https://github.com/tetarchus/utils.git"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    },
    "./client": {
      "import": {
        "default": "./dist/client/index.js",
        "types": "./dist/client/index.d.ts"
      }
    },
    "./core": {
      "import": {
        "default": "./dist/core/index.js",
        "types": "./dist/core/index.d.ts"
      }
    },
    "./remix": {
      "import": {
        "default": "./dist/remix/index.js",
        "types": "./dist/remix/index.d.ts"
      }
    },
    "./server": {
      "import": {
        "default": "./dist/server/index.js",
        "types": "./dist/server/index.d.ts"
      }
    },
    "./typeguards": {
      "import": {
        "default": "./dist/typeguards/index.js",
        "types": "./dist/typeguards/index.d.ts"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "npm:publish": "npm publish --access public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "lodash-es": "4.17.21"
  },
  "devDependencies": {
    "@remix-run/node": "2.3.0",
    "@types/lodash-es": "4.17.11",
    "@types/node": "20.9.3",
    "tsup": "8.0.1",
    "typescript": "5.3.2"
  }
}